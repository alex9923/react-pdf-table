{
  "name": "@alex9923/react-pdf-table",
  "version": "0.4.4-beta",
  "description": "Provides tables for the @react-pdf/renderer. But its updated to react version 18.2.0",
  "main": "./lib/index.js",
  "scripts": {
    "generate-barrels": "barrelsby -d src --delete -e TableRow.tsx",
    "build": "npm run generate-barrels && tsc",
    "recompile": "tsc",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "build-doco": "typedoc --plugin typedoc-plugin-markdown --out docs/typedoc --mode modules src"
  },
  "author": "David Kucsai",
  "license": "MIT",
  "peerDependencies": {
    "@react-pdf/renderer": "^3.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@react-pdf/renderer": "^4.1.5",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^8.0.1",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/addons": "^7.6.17",
    "@storybook/cli": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@types/enzyme": "^3.10.18",
    "@types/faker": "^6.6.11",
    "@types/node": "^22.10.2",
    "@types/sinon": "^17.0.3",
    "@types/storybook__react": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "barrelsby": "^2.8.1",
    "css-loader": "^7.1.2",
    "enzyme": "^3.11.0",
    "faker": "^6.6.6",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jsdom": "^25.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "react": "^19.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^19.0.0",
    "regenerator-runtime": "^0.14.1",
    "sinon": "^19.0.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.27.5",
    "typedoc-plugin-markdown": "^4.3.3",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "resolutions": {
    "dfa": "1.1.0"
  },
  "files": [
    "lib",
    "LICENSE",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex9923/react-pdf-table.git"
  },
  "bugs": {
    "url": "https://github.com/dmk99/react-pdf-table/issues"
  },
  "homepage": "https://github.com/dmk99/react-pdf-table.git#README",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "@storybook/addon-postcss": "^2.0.0"
  }
}
