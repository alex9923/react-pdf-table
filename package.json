{
  "name": "@alex9923/react-pdf-table",
  "version": "0.4.1-beta",
  "description": "Provides tables for the @react-pdf/renderer. But its updated to react version 18.2.0",
  "main": "./lib/index.js",
  "scripts": {
    "generate-barrels": "barrelsby -d src --delete -e TableRow.tsx",
    "build": "npm run generate-barrels && tsc",
    "recompile": "tsc",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "build-doco": "typedoc --plugin typedoc-plugin-markdown --out docs/typedoc --mode modules src"
  },
  "author": "David Kucsai",
  "license": "MIT",
  "peerDependencies": {
    "@react-pdf/renderer": "^3.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@react-pdf/renderer": "^3.1.9",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.7",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/cli": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/enzyme": "^3.10.12",
    "@types/faker": "^6.6.8",
    "@types/node": "^18.15.11",
    "@types/sinon": "^10.0.13",
    "@types/storybook__react": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "barrelsby": "^2.5.1",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "faker": "^6.6.6",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^21.1.1",
    "mini-css-extract-plugin": "^2.7.5",
    "react": "^18.2.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "sinon": "^15.0.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.0.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  },
  "resolutions": {
    "dfa": "1.1.0"
  },
  "files": [
    "lib",
    "LICENSE",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex9923/react-pdf-table.git"
  },
  "bugs": {
    "url": "https://github.com/dmk99/react-pdf-table/issues"
  },
  "homepage": "https://github.com/dmk99/react-pdf-table.git#README",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "@storybook/addon-postcss": "^2.0.0"
  }
